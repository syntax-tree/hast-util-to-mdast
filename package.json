{
  "name": "hast-util-to-mdast",
  "version": "2.0.0",
  "description": "Transform HAST to MDAST",
  "license": "MIT",
  "keywords": [
    "hast",
    "mdast",
    "rehype",
    "remark",
    "markdown",
    "html"
  ],
  "repository": "syntax-tree/hast-util-to-mdast",
  "bugs": "https://github.com/syntax-tree/hast-util-to-mdast/issues",
  "author": "Seth Vincent <sethvincent@gmail.com> (https://sethvincent.com)",
  "contributors": [
    "Seth Vincent <sethvincent@gmail.com> (https://sethvincent.com)",
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "dependencies": {
    "hast-util-has-property": "^1.0.0",
    "hast-util-is-element": "^1.0.0",
    "hast-util-to-string": "^1.0.0",
    "mdast-util-phrasing": "^1.0.0",
    "rehype-minify-whitespace": "^2.0.3",
    "trim-trailing-lines": "^1.1.0",
    "unist-util-is": "^2.1.0",
    "unist-util-visit": "^1.1.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "hastscript": "^3.0.1",
    "is-hidden": "^1.1.0",
    "mdast-util-assert": "^1.0.0",
    "negate": "^1.0.0",
    "nyc": "^11.0.0",
    "rehype-parse": "^4.0.0",
    "remark-cli": "^4.0.0",
    "remark-parse": "^4.0.0",
    "remark-preset-wooorm": "^3.0.0",
    "remark-stringify": "^4.0.0",
    "tape": "^4.6.3",
    "unified": "^6.1.0",
    "unist-builder": "^1.0.2",
    "unist-util-remove-position": "^1.1.0",
    "xo": "^0.18.1"
  },
  "scripts": {
    "build": "remark *.md -qfo",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "esnext": false,
    "rules": {
      "no-multi-assign": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}

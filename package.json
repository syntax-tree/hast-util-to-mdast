{
  "name": "hast-util-to-mdast",
  "version": "4.0.0",
  "description": "Transform hast to mdast",
  "license": "MIT",
  "keywords": [
    "hast",
    "mdast",
    "rehype",
    "remark",
    "markdown",
    "html"
  ],
  "repository": "syntax-tree/hast-util-to-mdast",
  "bugs": "https://github.com/syntax-tree/hast-util-to-mdast/issues",
  "author": "Seth Vincent <sethvincent@gmail.com> (https://sethvincent.com)",
  "contributors": [
    "Seth Vincent <sethvincent@gmail.com> (https://sethvincent.com)",
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "dependencies": {
    "hast-util-has-property": "^1.0.0",
    "hast-util-is-element": "^1.0.0",
    "hast-util-to-text": "^1.0.0",
    "mdast-util-phrasing": "^1.0.0",
    "mdast-util-to-string": "^1.0.4",
    "rehype-minify-whitespace": "^2.0.3",
    "trim-trailing-lines": "^1.1.0",
    "unist-util-visit": "^1.1.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "hastscript": "^5.0.0",
    "is-hidden": "^1.1.0",
    "mdast-util-assert": "^1.0.0",
    "negate": "^1.0.0",
    "nyc": "^13.0.0",
    "prettier": "^1.13.5",
    "rehype-parse": "^5.0.0",
    "remark-cli": "^6.0.0",
    "remark-parse": "^6.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "remark-stringify": "^6.0.0",
    "tape": "^4.6.3",
    "unified": "^7.0.0",
    "unist-builder": "^1.0.2",
    "unist-util-remove-position": "^1.1.0",
    "xo": "^0.23.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write '**/*.js' && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "no-multi-assign": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
